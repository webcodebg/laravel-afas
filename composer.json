{
    "name": "webcode/laravel-afas",
    "description": "Laravel package around tomlankhorst/afas-profit",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Lankhorst",
            "email": "tomlankhorst@webhub.nl"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "tomlankhorst\\LaravelAfas\\AfasServiceProvider"
            ],
            "aliases": {
                "Afas": "tomlankhorst\\LaravelAfas\\AfasFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "tomlankhorst\\LaravelAfas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tomlankhorst\\LaravelAfas\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "tomlankhorst/afas-profit": "^0.2.2",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|6.*",
        "illuminate/contracts": "5.5.*|5.6.*|5.7.*|5.8.*|6.*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^3.7"
    }
}
